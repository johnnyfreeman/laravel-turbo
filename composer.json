{
    "name": "johnnyfreeman/laravel-turbo",
    "description": "Hotwire Turbo integration for Laravel.",
    "keywords": ["laravel", "hotwire", "turbo"],
    "license": "MIT",
    "require": {
        "illuminate/container": "^10.3",
        "illuminate/contracts": "^10.3",
        "illuminate/support": "^10.3",
        "illuminate/view": "^10.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JohnnyFreeman\\LaravelTurbo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohnnyFreeman\\LaravelTurbo\\TurboServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
